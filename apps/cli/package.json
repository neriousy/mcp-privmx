{
  "name": "@privmx/mcp-cli",
  "version": "1.0.0",
  "description": "CLI tool for managing PrivMX MCP Server",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "privmx-mcp": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@privmx/mcp-server": "workspace:*",
    "@privmx/shared": "workspace:*",
    "commander": "^14.0.0",
    "inquirer": "^12.6.3",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
